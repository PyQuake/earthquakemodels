#!/usr/bin/env Rscript
setwd("~/Documents/estudos/unb/earthquakemodels/Zona2/")
options(scipen=999)
teste = function(region, year){
#GA DATA
file_GA_25 = paste("dataForR/",region,'_',year,"_Media25gaModel.txt",sep="")
raw_data_GA_25 = read.csv2(file_GA_25, sep='\t', header=F)
file_GA_25_STD = paste("dataForR/",region,'_',year,"_STD25gaModel.txt",sep="")
raw_data_GA_25_STD = read.csv2(file_GA_25_STD, sep='\t', header=F)
file_GA_60 = paste("dataForR/",region,'_',year,"_Media60gaModel.txt",sep="")
raw_data_GA_60 = read.csv2(file_GA_60, sep='\t', header=F)
file_GA_60_STD = paste("dataForR/",region,'_',year,"_STD60gaModel.txt",sep="")
raw_data_GA_60_STD = read.csv2(file_GA_60_STD, sep='\t', header=F)
file_GA_100 = paste("dataForR/",region,'_',year,"_Media100gaModel.txt",sep="")
raw_data_GA_100 = read.csv2(file_GA_100, sep='\t', header=F)
file_GA_100_STD = paste("dataForR/",region,'_',year,"_STD100gaModel.txt",sep="")
raw_data_GA_100_STD = read.csv2(file_GA_100_STD, sep='\t', header=F)
data25_GA = rep(0,99)
data25_GA_STD = rep(0, 99)
data60_GA = rep(0,99)
data60_GA_STD = rep(0, 99)
data100_GA = rep(0,99)
data100_GA_STD = rep(0, 99)
for (j in 1:99){
data25_GA[j] = data25_GA[j] + as.numeric(levels(raw_data_GA_25$V1[j+1])[raw_data_GA_25$V1[j+1]])
data25_GA_STD[j] = data25_GA_STD[j] + as.numeric(levels(raw_data_GA_25_STD$V1[j+1])[raw_data_GA_25_STD$V1[j+1]])
data60_GA[j] = data60_GA[j] + as.numeric(levels(raw_data_GA_60$V1[j+1])[raw_data_GA_60$V1[j+1]])
data60_GA_STD[j] = data60_GA_STD[j] + as.numeric(levels(raw_data_GA_60_STD$V1[j+1])[raw_data_GA_60_STD$V1[j+1]])
data100_GA[j] = data100_GA[j] + as.numeric(levels(raw_data_GA_100$V1[j+1])[raw_data_GA_100$V1[j+1]])
data100_GA_STD[j] = data100_GA_STD[j] + as.numeric(levels(raw_data_GA_100_STD$V1[j+1])[raw_data_GA_100_STD$V1[j+1]])
}
#lista data
file_lista_25 = paste("dataForR/",region,'_',year,"_Media25listaGA_New.txt",sep="")
raw_data_lista_25 = read.csv2(file_lista_25, sep='\t', header=F)
file_lista_25_STD = paste("dataForR/",region,'_',year,"_STD25listaGA_New.txt",sep="")
raw_data_lista_25_STD = read.csv2(file_lista_25_STD, sep='\t', header=F)
file_lista_60 = paste("dataForR/",region,'_',year,"_Media60listaGA_New.txt",sep="")
raw_data_lista_60 = read.csv2(file_lista_60, sep='\t', header=F)
file_lista_60_STD = paste("dataForR/",region,'_',year,"_STD60listaGA_New.txt",sep="")
raw_data_lista_60_STD = read.csv2(file_lista_60_STD, sep='\t', header=F)
file_lista_100 = paste("dataForR/",region,'_',year,"_Media100listaGA_New.txt",sep="")
raw_data_lista_100 = read.csv2(file_lista_100, sep='\t', header=F)
file_lista_100_STD = paste("dataForR/",region,'_',year,"_STD100listaGA_New.txt",sep="")
raw_data_lista_100_STD = read.csv2(file_lista_100_STD, sep='\t', header=F)
data25_lista = rep(0,99)
data25_lista_STD = rep(0, 99)
data60_lista = rep(0,99)
data60_lista_STD = rep(0, 99)
data100_lista = rep(0,99)
data100_lista_STD = rep(0, 99)
for (j in 1:99){
data25_lista[j] = data25_lista[j] + as.numeric(levels(raw_data_lista_25$V1[j+1])[raw_data_lista_25$V1[j+1]])
data25_lista_STD[j] = data25_lista_STD[j] + as.numeric(levels(raw_data_lista_25_STD$V1[j+1])[raw_data_lista_25_STD$V1[j+1]])
data60_lista[j] = data60_lista[j] + as.numeric(levels(raw_data_lista_60$V1[j+1])[raw_data_lista_60$V1[j+1]])
data60_lista_STD[j] = data60_lista_STD[j] + as.numeric(levels(raw_data_lista_60_STD$V1[j+1])[raw_data_lista_60_STD$V1[j+1]])
data100_lista[j] = data100_lista[j] + as.numeric(levels(raw_data_lista_100$V1[j+1])[raw_data_lista_100$V1[j+1]])
data100_lista_STD[j] = data100_lista_STD[j] + as.numeric(levels(raw_data_lista_100_STD$V1[j+1])[raw_data_lista_100_STD$V1[j+1]])
}
#graph ploting
file = paste("graphs/",region,'/',region,"_",year,"_depth25.png",sep="")
png(file)
par(mfrow=c(1,2))
x=1:99
plot(data25_GA, ylim=range(c(data25_GA-data25_GA_STD, data25_GA+data25_GA_STD)), pch=20,ylab='Loglikelihood',xlab='Generation',  col="blue")
arrows(x, data25_GA-data25_GA_STD, x, data25_GA+data25_GA_STD, length=0.05, angle=90, code=3)
legend("bottomright", "GAModel", pch = 20, col='blue', title = "bottomright")
plot(x, data25_lista, ylim=range(c(data25_lista-data25_lista_STD, data25_lista+data25_lista_STD)), pch=20,ylab='',xlab='Generation',  col="red")
arrows(x, data25_lista-data25_lista_STD, x, data25_lista+data25_lista_STD, length=0.05, angle=90, code=3)
legend("bottomright", "Listas", pch = 20, col='red', title = "bottomright")
text = paste("Log-likelihood/Generation, depth>25km in",region,'year:',year,sep=" ")
title(main=text, outer=T, line=-2)
dev.off()
file = paste("graphs/",region,'/',region,"_",year,"_depth60.png",sep="")
png(file)
par(mfrow=c(1,2))
x=1:99
plot(data60_GA, ylim=range(c(data60_GA-data60_GA_STD, data60_GA+data60_GA_STD)), pch=20,ylab='Loglikelihood',xlab='Generation',  col="blue")
arrows(x, data60_GA-data60_GA_STD, x, data60_GA+data60_GA_STD, length=0.05, angle=90, code=3)
legend("bottomright", "GAModel", pch = 20, col='blue', title = "bottomright")
plot(x, data60_lista, ylim=range(c(data60_lista-data60_lista_STD, data60_lista+data60_lista_STD)), pch=20,ylab='',xlab='Generation',  col="red")
arrows(x, data60_lista-data60_lista_STD, x, data60_lista+data60_lista_STD, length=0.05, angle=90, code=3)
legend("bottomright", "Listas", pch = 20, col='red', title = "bottomright")
text = paste("Log-likelihood/Generation, depth>60km in",region,'year:',year,sep=" ")
title(main=text, outer=T, line=-2)
dev.off()
file = paste("graphs/",region,'/',region,"_",year,"_depth100.png",sep="")
png(file)
par(mfrow=c(1,2))
x=1:99
plot(data100_GA, ylim=range(c(data100_GA-data100_GA_STD, data100_GA+data100_GA_STD)), pch=20,ylab='Loglikelihood',xlab='Generation',  col="blue")
arrows(x, data100_GA-data100_GA_STD, x, data100_GA+data100_GA_STD, length=0.05, angle=90, code=3)
legend("bottomright", "GAModel", pch = 20, col='blue', title = "bottomright")
plot(x, data100_lista, ylim=range(c(data100_lista-data100_lista_STD, data100_lista+data100_lista_STD)), pch=20,ylab='',xlab='Generation',  col="red")
arrows(x, data100_lista-data100_lista_STD, x, data100_lista+data100_lista_STD, length=0.05, angle=90, code=3)
legend("bottomright", "Listas", pch = 20, col='red', title = "bottomright")
text = paste("Log-likelihood/Generation, depth>100km in",region,'year:',year,sep=" ")
title(main=text, outer=T, line=-2)
dev.off()
}
for (year in 2000:2005){
teste("Kanto",year)
teste("EastJapan",year)
teste("Kansai",year)
teste("Tohoku",year)
}
